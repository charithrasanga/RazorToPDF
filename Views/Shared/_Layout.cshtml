     <style>
      .full-width-div {
        width: 100vw;
        position: relative;
        left: 50%;
        right: 50%;
        margin-left: -50vw;
        margin-right: -50vw;
      }
      .form-label {
        font-size: 14px;
        font-weight: 600;
      }
      .form-control {
        font-size: 14px;
        color: black;
        border-color: black;
        border-radius: 2px;
      }


input[type='checkbox']:after {
    box-shadow: none !important;
}
  
.form-check-input[type='checkbox'] {
    appearance: none; /* This hides the default checkbox */
    border: 1px solid black;
    border-radius: 2px !important;
}

.form-check-input[type='checkbox']:checked {
    background-color: transparent !important; /* This hides the default checkmark */
}

.form-check-input[type='checkbox']:checked:after {
    content: "";
    display: block;
    width: 6px;
    height: 12px;
    border: solid black;
    border-width: 0 2px 2px 0;
    transform: rotate(45deg);
    position: relative;
    left: 4px; /* Adjust this value to move the checkmark to the right */
}

      @@media print {
        .pagebreak {
          clear: both;
          page-break-after: always;
        }
      }

      .noto-color-emoji-regular {
        font-family: "Noto Color Emoji", sans-serif;
        font-weight: 400;
        font-style: normal;
      }

      html {
        -webkit-print-color-adjust: exact;
      }
    </style>
</head>
<body>
    <div class="container">

        @RenderBody()
    </div>
